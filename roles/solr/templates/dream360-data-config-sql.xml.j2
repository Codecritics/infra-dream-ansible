<dataConfig>
    <dataSource type="JdbcDataSource"
                driver="com.mysql.jdbc.Driver"
                url="jdbc:mysql://{{ ansible_facts.default_ipv4.address }}/{{ mysql_database }}"
                user="{{ mysql_user }}"
                password="{{ mysql_pass }}"/>
    <document>
        <entity name="item" pk='id'
                query="select id, sku, alerts_last_modified, title, description_txt, text_t, Typededoc_tech ,region,key_metadata,kpi,auth_groups,analyses_ventes  from biengine_360degrees"
                deltaImportQuery="select id, sku, alerts_last_modified, title, description_txt, text_t, Typededoc_tech ,region,key_metadata,kpi,auth_groups,analyses_ventes from biengine_360degrees where id='${dih.delta.id}'"
                deltaQuery="select id from biengine where last_modified &gt; '${dih.last_index_time}'">
            <field column="id" name="id"/>
            <field column="sku" name="sku"/>
            <field column="alerts_last_modified" name="alerts_last_modified"/>
            <field column="title" name="title"/>
            <field column="description_txt" name="key"/>
            <field column="text_t" name="text_t"/>
            <field column="Typededoc_tech" name="Typededoc_tech"/>
            <field column="region" name="region"/>
            <field column="key_metadata" name="key_metadata"/>
            <field column="kpi" name="kpi"/>
            <field column="auth_groups" name="auth_groups"/>
            <field column="analyses_ventes" name="analyses_ventes"/>
        </entity>
    </document>
</dataConfig>
